    "@uppy/react": "3.4.0",
    "@uppy/status-bar": "3.3.3",
    "@uppy/tus": "3.5.5",
    "@uppy/unsplash": "3.3.1",
    "@uppy/url": "3.6.1",
    "@uppy/webcam": "3.4.2",
    "preact": "10.20.1"
  },
  "devDependencies": {
