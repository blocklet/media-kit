{
  "name": "@blocklet/uploader",
  "version": "0.1.27",
  "description": "blocklet upload component",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "import": "./es/index.js",
      "types": "./lib/index.d.ts"
    },
    "./middlewares": {
      "require": "./lib/middlewares.js",
      "import": "./es/middlewares.js",
      "types": "./lib/middlewares.d.ts"
    },
    "./react": {
      "require": "./lib/react.js",
      "import": "./es/react.js",
      "types": "./lib/react.d.ts"
    },
    "./lib/": "./lib/",
    "./es/": "./es/"
  },
  "files": [
    "lib",
    "es",
    "*.d.ts"
  ],
  "scripts": {
    "coverage": "pnpm test -- --coverage",
    "test": "vitest tests",
    "build": "unbuild",
    "build:watch": "npx nodemon --ext 'ts,tsx,json,js,jsx' --exec 'pnpm run build' --ignore 'lib/*' --ignore 'es/*' ",
    "dev": "pnpm run build:watch",
    "prepublish": "pnpm run build",
    "prebuild:dep": "pnpm run build"
  },
  "keywords": [
    "blocklet",
    "uploader"
  ],
  "author": "arcblock <blocklet@arcblock.io> https://github.com/blocklet",
  "license": "ISC",
  "dependencies": {
    "@abtnode/cron": "^1.16.30",
    "@blocklet/constant": "^1.16.30",
    "@blocklet/logger": "^1.16.30",
    "@blocklet/sdk": "^1.16.30",
    "@blocklet/ui-react": "^2.10.27",
    "@mui/icons-material": "^5.15.15",
    "@mui/material": "^5.15.15",
    "@tus/file-store": "1.0.0",
    "@tus/server": "1.0.0",
    "@uppy/companion": "4.13.2",
    "@uppy/core": "3.11.2",
    "@uppy/dashboard": "3.8.3",
    "@uppy/drag-drop": "3.1.0",
    "@uppy/drop-target": "2.1.0",
    "@uppy/file-input": "3.1.2",
    "@uppy/golden-retriever": "3.2.0",
    "@uppy/image-editor": "2.4.5",
    "@uppy/locales": "3.5.3",
    "@uppy/progress-bar": "3.1.1",
    "@uppy/provider-views": "3.12.0",
    "@uppy/react": "3.3.1",
    "@uppy/status-bar": "3.3.3",
    "@uppy/tus": "3.5.4",
    "@uppy/unsplash": "3.3.1",
    "@uppy/url": "3.6.1",
    "@uppy/webcam": "3.4.1",
    "ahooks": "^3.7.11",
    "axios": "^1.6.8",
    "body-parser": "^1.20.2",
    "crypto": "^1.0.1",
    "dompurify": "^3.1.6",
    "lodash": "^4.17.21",
    "lottie-react": "^2.4.0",
    "mime-types": "^2.1.35",
    "p-queue": "^6.6.2",
    "preact": "10.20.1",
    "react-player": "^2.16.0",
    "ufo": "^1.5.3",
    "url-join": "^4.0.1",
    "xbytes": "^1.9.1"
  },
  "devDependencies": {
    "@arcblock/eslint-config-ts": "^0.2.4",
    "@types/express": "^4.17.21",
    "@types/lodash": "^4.17.0",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@types/url-join": "^4.0.3",
    "@vitest/coverage-c8": "^0.33.0",
    "jsdom": "^22.1.0",
    "typescript": "^5.4.5",
    "unbuild": "^2.0.0",
    "vitest": "^1.5.0",
    "vitest-fetch-mock": "^0.2.2"
  }
}
