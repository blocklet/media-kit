{
  "name": "image-bin",
  "version": "0.14.11",
  "private": true,
  "scripts": {
    "dev": "blocklet dev",
    "build": "vite build",
    "lint": "eslint src api --ext .mjs,.js,.jsx,.ts,.tsx",
    "start": "NODE_ENV=development nodemon api/dev.js -w api",
    "clean": "rm -rf .blocklet",
    "bundle": "npm run bundle:client && npm run bundle:api",
    "bundle:client": "npm run build",
    "bundle:api": "npm run clean && blocklet bundle --compact --create-release --external sqlite3,@aigne/sqlite",
    "deploy": "npm run bundle && blocklet deploy .blocklet/bundle",
    "upload": "npm run bundle && blocklet upload .blocklet/release/blocklet.json",
    "update:deps": "npx taze -n '/@aigne|arcblock|ocap|abtnode|blocklet|did-connect|did-comment|nedb/' -w -f && pnpm i && pnpm run deduplicate",
    "studio": "COMPONENT_STORE_URL=https://test.store.blocklet.dev blocklet dev studio",
    "remove:node_modules": "rm -rf ./node_modules ./blocklets/*/node_modules ./packages/*/node_modules ./contracts/*/node_modules",
    "deduplicate": "pnpm dedupe",
    "bundle:analyze": "pnpx vite-bundle-visualizer -o stats.html --open"
  },
  "lint-staged": {
    "*.{mjs,js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint"
    ],
    "*.{css,less,scss,json,graphql}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@aigne/aigne-hub": "^0.10.11-beta",
    "@arcblock/did": "^1.27.11",
    "@arcblock/did-connect-react": "^3.2.8",
    "@arcblock/ux": "^3.2.8",
    "@blocklet/constant": "^1.17.3-beta-20251123-232619-53258789",
    "@blocklet/embed": "^0.3.3",
    "@blocklet/js-sdk": "^1.17.3-beta-20251123-232619-53258789",
    "@blocklet/logger": "^1.17.3-beta-20251123-232619-53258789",
    "@blocklet/sdk": "^1.17.3-beta-20251123-232619-53258789",
    "@blocklet/uploader": "workspace:^",
    "@blocklet/uploader-server": "workspace:^",
    "@blocklet/xss": "workspace:^",
    "@iconify/react": "^5.0.2",
    "@mui/icons-material": "^7.1.2",
    "@mui/material": "^7.1.2",
    "@ocap/client": "^1.27.11",
    "compression": "^1.7.5",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dompurify": "^3.2.2",
    "dotenv-flow": "^3.3.0",
    "express": "^4.21.1",
    "express-async-errors": "^3.1.1",
    "lru-cache": "^11.0.2",
    "mime-types": "^2.1.35",
    "multer": "1.4.5-lts.1",
    "nanoid": "^3.3.8",
    "react-player": "^2.16.0",
    "semver": "^7.6.3",
    "sequelize": "^6.37.7",
    "serve-static": "^1.16.2",
    "sqlite3": "^5.1.7",
    "ufo": "^1.5.4",
    "umzug": "^3.8.2",
    "url-join": "^4.0.1",
    "xbytes": "^1.9.1"
  },
  "devDependencies": {
    "@arcblock/eslint-config": "^0.3.3",
    "@blocklet/ui-react": "^3.2.8",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@vitejs/plugin-react": "^4.6.0",
    "ahooks": "^3.8.1",
    "axios": "^1.7.8",
    "code-inspector-plugin": "^1.2.10",
    "copy-to-clipboard": "^3.3.3",
    "eslint": "^8.57.1",
    "lint-staged": "^13.3.0",
    "lodash": "^4.17.21",
    "nodemon": "^2.0.22",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "pretty-bytes": "^6.1.1",
    "prop-types": "^15.8.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^6.28.0",
    "react-use": "^17.5.1",
    "swiper": "^9.4.1",
    "timeago.js": "^4.0.2",
    "vite": "^7.0.0",
    "vite-plugin-blocklet": "^0.12.3",
    "vite-plugin-svgr": "^4.3.0",
    "wolfy87-eventemitter": "^5.2.9"
  }
}
