{
  "name": "image-bin",
  "version": "0.10.4",
  "scripts": {
    "dev": "blocklet dev",
    "build": "vite build",
    "lint": "eslint src api --ext .mjs,.js,.jsx,.ts,.tsx",
    "start": "NODE_ENV=development nodemon api/dev.js -w api",
    "clean": "rm -rf .blocklet",
    "bundle": "npm run bundle:client && npm run bundle:api",
    "bundle:client": "npm run build",
    "bundle:api": "npm run clean && blocklet bundle --zip --create-release",
    "deploy": "npm run bundle && blocklet deploy .blocklet/bundle",
    "upload": "npm run bundle && blocklet upload .blocklet/release/blocklet.json",
    "update:deps": "npx taze -n '/arcblock|ocap|abtnode|blocklet|did-connect|did-comment|nedb/' -w && pnpm i && pnpm dedupe"
  },
  "lint-staged": {
    "*.{mjs,js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint"
    ],
    "*.{css,less,scss,json,graphql}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@blocklet/embed": "^0.1.5",
    "@blocklet/logger": "^1.16.15",
    "@blocklet/sdk": "^1.16.15",
    "@blocklet/uploader": "workspace:^",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv-flow": "^3.3.0",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "mime-types": "^2.1.35",
    "multer": "^1.4.5-lts.1",
    "nanoid": "^3.3.6",
    "serve-static": "^1.15.0",
    "sharp": "^0.32.5",
    "url-join": "^4.0.1",
    "xbytes": "^1.8.0"
  },
  "devDependencies": {
    "@arcblock/did-connect": "^2.7.17",
    "@arcblock/eslint-config": "^0.2.4",
    "@arcblock/ux": "^2.7.17",
    "@blocklet/ui-react": "^2.7.17",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.14.8",
    "@vitejs/plugin-react": "^4.0.4",
    "ahooks": "^3.7.8",
    "axios": "^1.5.0",
    "copy-to-clipboard": "^3.3.3",
    "eslint": "^8.49.0",
    "lint-staged": "^13.3.0",
    "lodash": "^4.17.21",
    "nodemon": "^2.0.22",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "pretty-bytes": "^6.1.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.15.0",
    "react-use": "^17.4.0",
    "swiper": "^9.4.1",
    "timeago.js": "^4.0.2",
    "vite": "^4.4.9",
    "vite-plugin-blocklet": "^0.6.15",
    "wolfy87-eventemitter": "^5.2.9"
  },
  "resolutions": {
    "caniuse-lite": "1.0.30001373"
  }
}
